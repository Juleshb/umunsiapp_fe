# Chart App Client Environment Variables

# API Configuration
VITE_API_URL=http://localhost:5002/api

# Socket Configuration
VITE_SOCKET_URL=http://localhost:5002

# PWA Configuration
VITE_VAPID_PUBLIC_KEY=your_vapid_public_key_here

# App Configuration
VITE_APP_NAME=Chart App
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=A modern social media platform with chat, clubs, articles, and community features

# Feature Flags
VITE_ENABLE_PUSH_NOTIFICATIONS=true
VITE_ENABLE_OFFLINE_MODE=true
VITE_ENABLE_BACKGROUND_SYNC=true

# Analytics (optional)
VITE_GOOGLE_ANALYTICS_ID=your_ga_id_here

# Social Media
VITE_TWITTER_HANDLE=@chartapp
VITE_FACEBOOK_PAGE=https://facebook.com/chartapp

# Support
VITE_SUPPORT_EMAIL=support@chartapp.com
VITE_SUPPORT_URL=https://chartapp.com/support 